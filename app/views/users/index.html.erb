<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="digg_pagination">
  <%= will_paginate %>
</div>



<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>

<div class="digg_pagination">
  <%= will_paginate %>
</div>
